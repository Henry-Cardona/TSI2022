
pipeline {
    agent any 
    stages {
        stage('test') { 
            steps {
                dir("TSI_WEB_AccessData"){
                    bat "mvn test"
                }
            }
        }
        stage('verify'){
            steps{
                dir("TSI_WEB_AccessData"){
                    bat "mvn -f pom.xml -Parquillian-payara clean verify"
                }
            }
        }
        stage('test'){
            steps{
                dir("TSI_WEB_AccessData"){
                    bat "mvn clean test"
                }
            }
        }
        stage('package'){
            steps{
                dir("TSI_WEB_AccessData"){
                    bat "mvn clean package"
                }
            }
        }
    }
}
